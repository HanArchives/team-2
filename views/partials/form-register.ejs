<section class="register">
    <h2>Register</h2>
    <img class="img-add-doggo" src="img/ui/mainpom.png" alt="Main pom logo">
    <form action="/register" method="get" enctype="multipart/form-data">
        <label class="label" for="firstname">First name:</label>
        <input type="text" name="firstname" placeholder="First name">

        <label class="label" for="lastname">Last name:</label>
        <input type="text" name="lastname" placeholder="Last name">

        <label class="label" for="email">Email:</label>
        <input type="email" name="email" placeholder="Email">

        <label class="label" for="password">Password:</label>
        <input type="password" name="password" placeholder="Password">

        <label class="label" for="password2">Confirm password:</label>
        <input type="password" name="password2" placeholder="Confirm password">

        <button type="submit" value="continue">Submit</button>

    </form>
</section>